@model IEnumerable<AssignmentOnMVCUsingViewModel.Models.Product>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
 }
    <a href="Create">Create</a>
    <div class="container">
        <table class="table table-bordered">
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Price</th>
                <th>Stock</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Pid</td>
                    <td>@item.Pname</td>
                    <td>@item.Pprice</td>
                    <td>@item.Pstock</td>
                </tr>
            }
        </table>
    </div>